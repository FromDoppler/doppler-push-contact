{
  "LogglyConfig": {
    "CustomerToken": "REPLACE_WITH_CUSTOMER_TOKEN"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "DopplerSecurity": {
    "PublicKeysFolder": "public-keys",
    "PublicKeysFilenameRegex": "\\.xml$"
  },
  "PushContactMongoContextSettings": {
    "MongoConnectionString": "REPLACE_WITH_MONGO_CONNECTION_STRING",
    "MongoPushContactDatabaseName": "REPLACE_WITH_MONGO_PUSH_CONTACT_DATABASE_NAME",
    "MongoPushContactCollectionName": "REPLACE_WITH_MONGO_PUSH_CONTACT_COLLECTION_NAME"
  }
}
